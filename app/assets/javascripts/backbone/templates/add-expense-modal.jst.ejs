<div class="modal fade" tabindex="-1" role="dialog" id="expense-modal">
  <div class="modal-dialog modal-size-350" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">Add expense</h4>
      </div>
      <div class="modal-body">
        <!-- <p> Body</p> -->
        <form class="form-horizontal" name="add-expense">
          <div class="form-group">
            <label for="add-expense-name" class="col-sm-2 control-label">Name:</label>
            <div class="col-sm-10">
              <input type="text" name="name" id="add-expense-name" class="form-control" placeholder="Enter Name" />
            </div>
          </div>
          <div class="form-group">
            <label for="add-expense-val" class="col-sm-2 control-label">Cost:</label>
            <div class="col-sm-10">
              <input type="text" name="cost" id="add-expense-val" class="form-control" placeholder="Enter Value" />
            </div>
          </div>
          <button type="submit" class="btn btn-success pull-right mdB msT">Submit</button>
        </form>
      </div>
      <div class="clearfix"></div>
    </div>
  </div>
</div>
